{%extends "base.html.twig" %}
{% block body %}
<div class="col-sm-9">
<iframe style="width: 100%; height: 500px" src="https://www.youtube.com/embed/{{yt.url|getYoutubeId }}" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
<div>
    <h2>{{ yt.name }}</h2>
    <p>{{ yt.description }}</p>
</div>
</div>
{% endblock %}