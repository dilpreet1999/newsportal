                                In <a href="{{
                                  path(
                                    'story_by_category',
                                    {
                                      id: p.category.id,
                                      slug: p.category.name
                                    }
                                  )
                                  }}"
                                  style="padding: 2px 10px 2px 10px;color: white"
                                  class="cmsmasters_cat_color cmsmasters_cat_27"
                                  rel="category tag">
                                  {{ p.category.name }}
                                </a>
                              </span>
                                            </div>
                                            <header class="cmsmasters_archive_item_header entry-header">
                                                <h2 class="cmsmasters_archive_item_title entry-title">
                                                    <a href="{{path('story_view',{'id': p.id,'slug': p.title|slug})}}">
                                  {{ p.title }}
                                </a>
                                                </h2>
                                            </header>
                                            <div class="cmsmasters_archive_item_content entry-content">
                                                <p>
                                                    {% set description = wordWrap(p.body) %} {{ description|raw }}
                                                </p>
                                            </div>
                                        </div>
                                    </article>
                                    <div class="cmsmasters_wrap_pagination"></div>
                                </div>
                            </div>
                            {% endfor %}
                            <!-- Finish Content -->
                        </div>
                    </div>
                    {% else %}
                    <p>
                        No Result Found
                    </p>
                    {% endif %}
                </div>
                <div class="col-md-3">
                    <div class="right-sec">
                        <div class="heading">
                            <h3>
                                Most Viewed
                            </h3>
                        </div>
                        <div class="footer-blogs">
                            {% for mv in mostViewed %} {% set img = mv.id|getStoryImg %}
                            <div class="box">
                                <div class="images">
                                    {% if img %}
                                    <img style="width: 60px;" src="{{ path('story_images_view',{'id':img.id,'size':360,'name':img.fileName}) }}" /> {% else %}
                                    <img style="width: 60px;" src="{{ asset('') }}images/3.jpg" /> {% endif %}
                                </div>
                                <div class="content">
                                    <span>January 01,2022</span>
                                    <a href="{{path('story_view',{'id': mv.id,'slug': mv.title|slug})}}">
                                        <h4>
                                            {{ mv.title }}
                                        </h4>
                                    </a>
                                </div>
                            </div>
                            {% endfor %}
                        </div>
                    </div>
                    <div class="Subscribe">
                        <h2>
                            Subscribe to Our Newsletter
                        </h2>
                        <form>
                            <div class="form-group">
                                <input type="email" class="form-control" placeholder="Enter Your Email...*" />
                            </div>
                            <button type="button" class="form-control">SUBSCRIBE</button>
                        </form>
                        <p>
                            *we hate spam as much as you do
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
