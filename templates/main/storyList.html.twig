                                path(
                                  'story_view',
                                  {
                                    id: p.id,
                                    slug: p.title|slug
                                  }
                                )
                                }}" title="{{ p.title }}" class="cmsmasters_img_link preloader">
                                {% if storyImg  %}
                                <img width="360" height="360" src="{{ path('story_images_view',{'id':storyImg.id,'size':360,'name':storyImg.fileName}) }}" class="full-width wp-post-image" alt="blog6" loading="lazy" title="blog6" srcset="{{ path('story_images_view',{'id':storyImg.id,'size':360,'name':storyImg.fileName}) }} 360w, {{ path('story_images_view',{'id':storyImg.id,'size':360,'name':storyImg.fileName}) }} 150w, {{ path('story_images_view',{'id':storyImg.id,'size':360,'name':storyImg.fileName}) }} 120w, {{ path('story_images_view',{'id':storyImg.id,'size':360,'name':storyImg.fileName}) }} 60w"
                                                        sizes="(max-width: 360px) 100vw, 360px" />
                                                        {% else %}
