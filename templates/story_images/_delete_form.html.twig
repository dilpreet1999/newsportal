<form method="post" action="{{ path('story_images_delete', {'id': story_image.id,'storyId':storyId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ story_image.id) }}">
    <button class="btn">Delete</button>
</form>
